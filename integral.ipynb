    "from sympy import symbols, integrate\n",
    "import sympy\n",
    "from IPython.display import display,Math\n",
    "\n",
    "x = symbols('x')\n",
    "# 计算 ∫x² dx\n",
    "result = integrate(x**2, x)\n",
    "print(result)  # 输出: x**3/3"
   ]
